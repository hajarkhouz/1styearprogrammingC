/*Écrire un programme qui lit un entier A et affecte sa valeur à un entier B, puis affiche A et B. */


#include<stdio.h>
#include<stdlib.h>

int main(){
    int a, b;
    printf("Saisir la valeur de a : ");
    scanf( "%d", &a);
    b=a;
    printf("a = %d et b = %d\n", a, b);    
    system("pause");
    return 0;
}

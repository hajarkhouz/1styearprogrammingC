/* Écrire un programme qui calcule le double d’un nombre entier quelconque. */
/*Écrire un programme qui calcule le carré d’un nombre entier quelconque. */
#include<stdio.h>
#include<stdlib.h>

int main(){
    int a, b, c;
    printf("Saisir a :");
    scanf("%d", &a);
    b = 2 * a;
    c = a * a;
    printf("b=%d et c=%d\n", b, c);
    system("pause");
    return 0;
}
